{
  "name": "dailyquestions",
  "displayName": "Daily Questions",
  "expo": {
    "name": "Daily Questions",
    "slug": "daily-questions",
    "description": "Marshall Goldsmith's Daily Engagement Questions as an inofficial app",
    "orientation": "default",
    "version": "1.13.0",
    "assetBundlePatterns": ["**/*"],
    "privacy": "public",
    "android": {
      "package": "eu.kraenz.dailyquestions",
      "permissions": ["RECEIVE_BOOT_COMPLETED", "SCHEDULE_EXACT_ALARM"],
      "versionCode": 13,
      "adaptiveIcon": {
        "foregroundImage": "./assets/dailies-logo.png"
      },
      "icon": "./assets/dailies-logo_legacy.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 10000
    },
    "icon": "./assets/dailies-logo.png",
    "splash": {
      "image": "./assets/dailies-splash-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#141414"
    },
    "ios": {
      "bundleIdentifier": "eu.kraenz.dailyquestions"
    },
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png"
        }
      ]
    ]
  }
}
