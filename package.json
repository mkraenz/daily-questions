    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.3",
    "expo": "^49.0.0",
    "expo-clipboard": "~4.3.1",
    "expo-constants": "~14.4.2",
    "expo-crypto": "~12.4.1",
    "expo-localization": "~14.3.0",
    "expo-notifications": "~0.20.1",
    "expo-permissions": "~14.2.1",
