    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "^11.18.0",
    "react-native": "0.70.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-draggable-flatlist": "^3.1.2",
    "react-native-gesture-handler": "~2.8.0",
