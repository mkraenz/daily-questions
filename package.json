    "fuse.js": "^6.6.2",
    "i18next": "^21.8.13",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^11.18.0",
    "react-native": "0.71.4",
    "react-native-chart-kit": "^6.12.0",
